{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
      "no-restricted-globals": "warn",
      "no-bitwise": ["error", {"allow": ["|","&",">>>",">>","<<"]}],
      "no-plusplus": ["error", {"allowForLoopAfterthoughts": true}]
    }
}
